/* === Gram√°tica para expresiones con SLR(1) === */

%token ID
%token PLUS
%token TIMES
%token LPAREN RPAREN
%token SEMICOLON
%token WHITESPACE
IGNORE WHITESPACE



input:
    general SEMICOLON
;

general:
    general SEMICOLON expression
  | expression
;

expression:
    expression PLUS term
  | term
;

term:
    term TIMES factor
  | factor
;

factor:
    LPAREN expression RPAREN
  | ID
;